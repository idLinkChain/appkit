  "homepage": "https://github.com/WalletConnect/web3modal",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WalletConnect/web3modal.git"
  },
  "bugs": {
    "url": "https://github.com/WalletConnect/web3modal/issues"
  }
